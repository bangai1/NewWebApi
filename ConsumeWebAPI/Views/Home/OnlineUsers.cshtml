@{
    ViewData["Title"] = "Online Users";
}
<h1>@ViewData["Title"]</h1>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Current Users</title>
    <link rel="stylesheet" href="users.css">
</head>
<body>
    <div class="users-container">
        <h1>Current Users</h1>
        <div class="users-list">
            <div class="user-card">
                <img src="https://via.placeholder.com/80" alt="User Avatar">
                <div class="user-info">
                    <h3>John Doe</h3>
                    <p>Online</p>
                </div>
            </div>
            <div class="user-card">
                <img src="https://via.placeholder.com/80" alt="User Avatar">
                <div class="user-info">
                    <h3>Jane Smith</h3>
                    <p>Offline</p>
                </div>
            </div>
            <div class="user-card">
                <img src="https://via.placeholder.com/80" alt="User Avatar">
                <div class="user-info">
                    <h3>Michael Johnson</h3>
                    <p>Online</p>
                </div>
            </div>
        </div>
    </div>
</body>
</html>